Syntax error: unknown type "[" in variable declaration of "ջ"
test/labs/lab7-3.pas ==================================================================
{{{
  1 program p3;
  2 
  3 const lmax = 40;
  4 
  5 type
  6   tv = record
  7     manufacturer: record
  8       city, factory: String;
  9     end;
 10     name: String;
 11     params: record
 12       year, price: Integer;
 13       size: Real;
 14     end;
 15   end;
 16 
 17 var
 18   records: array [1..lmax] of tv;
 19   nr, i, searchedIdx, maxYear, minPrice: Integer;
 20   f: File of tv;
 21   name: String;
 22 
 23 begin
 24   writeln('Лабораторная работа №7');
 25   writeln('Акбашев Руслан, группа БИВ155, вариант 141, задание 3 (12)');
 26 
 27   repeat
 28     write('Имя файла для записи: ');
 29     readln(name);
 30   until CanCreateFile(name);
 31   
 32   assign(f, name);
 33   rewrite(f);
 34   
 35   repeat
 36     write('Количество записей: ');
 37     readln(nr);
 38   until nr in [1..lmax];
 39   
 40   writeln('Введите данные:');
 41   for i := 1 to nr do begin
 42     with records[i] do begin
 43       with manufacturer do begin
 44         write('Город изготовителя: ');
 45         readln(city);
 46         write('Завод: ');
 47         readln(factory);
 48       end;
 49       write('Название: ');
 50       readln(name);
 51       with params do begin
 52         repeat
 53           write('Год выпуска: ');
 54           readln(year);
 55         until (year > 1900) and (year <= 2015);
 56 
 57         repeat
 58           write('Цена: ');
 59           readln(price);
 60         until price > 0;
 61 
 62         repeat
 63           write('Размер экрана: ');
 64           readln(size);
 65         until size > 0;
 66         writeln;
 67       end;
 68     end;
 69     write(f, records[i]);
 70   end;
 71   close(f);
 72   
 73   searchedIdx := 0;
 74   maxYear := records[1].params.year;
 75   minPrice := 0;
 76   for i := 1 to nr do
 77     with records[i], params do
 78       if (year >= maxYear) or (maxYear = 0) then
 79         maxYear := year;
 80   for i := 1 to nr do
 81     with records[i], params do
 82       if ((price < minPrice) or (minPrice = 0)) and (year = maxYear) then begin
 83         minPrice := price;
 84         searchedIdx := i
 85       end;
 86 
 87   writeln('Искомый телевизор: ', records[searchedIdx].name);
 88 end.
}}}
