Generated code: {{{
/* Program "TRI-HEAP" */
#include <stdio.h>

const int N = 10;

int TEMP;

}}}
test/labs/lab1.pas ==================================================================
{{{
  1 program lab1;
  2 const lmax = 20;
  3 var B, D, A: array[1..lmax] of real;
  4     i, k, n, nmax: integer;
  5     maximum, z, x, z1, z2, x1, y1: real;
  6 
  7 begin
  8   writeln('  1');
  9   writeln(' ,  155,  141, : 14, 2, 4');
 10   
 11   { ###  1 }
 12   writeln(' 1');
 13   repeat
 14     write('   k  1  ', lmax);
 15     write(',   B: ');
 16     read(k);
 17   until (k > 0) and (k < lmax);
 18   
 19   writeln('  B  ', k, ' :');
 20   for i := 1 to k do
 21     read(B[i]);
 22 
 23   repeat
 24     write('   x  z,  x <= z: ');
 25     read(x, z);
 26   until (x <= 0) and (z <= 0) and (z >= x);
 27   
 28   maximum := 0;
 29   nmax := 0;
 30   for i := 1 to k do
 31     if (abs(B[i]) >= abs(maximum)) and
 32        (-z <= abs(B[i])) and
 33        (abs(B[i]) <= -x) then
 34     begin
 35       maximum := B[i];
 36       nmax := i;
 37     end;
 38     
 39   if nmax = 0 then
 40     writeln(' .')
 41   else
 42     writeln(' = ', maximum, ',     = ', nmax);
 43   
 44   { ###  2 }
 45   writeln;
 46   writeln(' 2');
 47   repeat
 48     write('  z1  z2,  z1 <= z2: ');
 49     read(z1, z2);
 50   until (z1 <= z2);
 51   
 52   n := 0;
 53   for i := 1 to k do
 54     if (z1 <= B[i]) and (B[i] <= z2) then
 55     begin
 56       n := n + 1;
 57       D[n] := B[i];
 58     end;
 59     
 60   if n = 0 then
 61     writeln('    D  A.')
 62   else
 63   begin
 64     writeln(' D  ', n, ' ');
 65     for i := 1 to n do
 66       write(D[i]:10:3, ' ');
 67     writeln;
 68     
 69     { ###  3 }
 70     writeln;
 71     writeln(' 3');
 72     repeat
 73       write('  x1  y1,  x1 < y1: ');
 74       read(x1, y1);
 75     until (x1 < y1);
 76     
 77     for i := 1 to n do
 78       if (-y1 < D[i]) and (D[i] < -x1) then
 79         if (D[i] < 0) then begin
 80           A[i] := 0;
 81           write('  A[', i, ']  ',
 82               ',   ')
 83         end else
 84           A[i] := sqrt(D[i])
 85       else
 86         A[i] := D[i] / 3;
 87         
 88     writeln(' A  ', n, ' ');
 89     for i := 1 to n do
 90       write(A[i]:10:3);
 91     writeln;
 92   end;
 93 end.
}}}
<parsed program>

