test/labs/lab8.pas ==================================================================
{{{
  1 program woop;
  2 
  3 type nodeptr = ^node;
  4   node = record
  5     next: nodeptr;
  6     data: integer;
  7   end;
  8 
  9 var root: ^node;
 10 
 11 procedure print(var root: ^node);
 12 var p: ^node;
 13 begin
 14   p := root;
 15   while true do begin
 16     write(p^.data, ' -> ');
 17     if p^.next = nil then begin
 18       writeln('end');
 19       break;
 20     end;
 21     p := p^.next
 22   end;
 23 end;
 24   
 25 procedure make_queue(var root: ^node);
 26 var
 27   t: integer;
 28   p, temp: ^node;
 29 begin
 30   writeln('    :');
 31   writeln('(-1   )');
 32   read(t);
 33   if t = -1 then
 34     root := nil
 35   else begin
 36     new(root);
 37     temp := root;
 38     repeat
 39       p := temp;
 40       new(temp);
 41       p^.next := temp;
 42       p^.data := t;
 43       read(t);
 44     until t = -1;
 45     p^.next := nil;
 46     dispose(temp);
 47   end;
 48 end;
 49 
 50 procedure process(var root: ^node);
 51 var p, t: ^node; i, a1: integer;
 52 begin
 53   p := root;
 54   i := 1;
 55   write('  A1: ');
 56   read(a1);
 57   while true do begin
 58     if i mod 2 = 1 then begin
 59       t := p^.next;
 60       new(p^.next);
 61       p^.next^.next := t;
 62       p^.next^.data := a1;
 63     end;
 64     if p^.next = nil then
 65       break;
 66     p := p^.next;
 67     inc(i);
 68   end;
 69 end;
 70 
 71 procedure removals(var root: ^node);
 72 var p, t: ^node; i, f, l: integer;
 73 begin
 74   p := root;
 75   f := 0;
 76   i := 0;
 77   while true do begin
 78     inc(i);
 79     if p^.data mod 2 = 0 then begin
 80       if f = 0 then
 81         f := i;
 82       l := i;
 83     end;
 84     if p^.next = nil then
 85       break;
 86     p := p^.next;
 87   end;
 88   if f = 0 then
 89     writeln('     ')
 90   else if f = l then
 91     writeln('      ')
 92   else begin
 93     p := root;
 94     i := 0;
 95     while true do begin
 96       inc(i);
 97       if i = f then
 98         break;
 99       p := p^.next;
100     end;
101     while i <> l-1 do begin
102       inc(i);
103       t := p^.next^.next;
104       dispose(p^.next);
105       p^.next := t;
106     end;
107   end;
108 end;
109 
110 begin
111   make_queue(root);
112   if root = nil then
113     writeln(' ')
114   else begin
115     print(root);
116     process(root);
117     print(root);
118     removals(root);
119     print(root);
120   end;
121 end.
}}}
