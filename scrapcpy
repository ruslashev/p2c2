{{{
  1 program lab3_prog1;
  2 var i: integer;
  3     s, prev, cur, eps, x: real;
  4 
  5 begin
  6   writeln('  3  1');
  7   writeln(' ,  155,  141,   6');
  8   writeln(' 1');
  9   
 10   repeat
 11     write('   eps  : ');
 12     read(eps);
 13   until (eps > 0);
 14   repeat
 15     write('  x   : ');
 16     read(x);
 17   until (x > 0);
 18   
 19   prev := (x - 1)/x;
 20   cur := sqr(x - 1)/(2*sqr(x));
 21   s := prev + cur;
 22   i := 2;
 23   while abs(cur) > eps do
 24   begin
 25     prev := cur;
 26     cur := prev * (i - 1)*(x - 1)/(i*x);
 27     s := s + cur;
 28     i := i + 1
 29   end;
 30   writeln('   : ', s);
 31   writeln(' : ln(', x, ') = ', ln(x));
 32   writeln(': |ln(x)-s| = ', abs(ln(x)-s));
 33   writeln('  ', 100*(1-s/ln(x)) : 7 : 6, '%');
 34 end.
}}}
