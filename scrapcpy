test/labs/lab3-2.pas ==================================================================
{{{
  1 program lab3_prog2;
  2 const lmax = 20;
  3 var A: array[1..lmax, 1..lmax] of integer;
  4     i, j, n, t, nodd: integer;
  5 
  6 begin
  7   writeln('  3  2');
  8   writeln(' ,  155,  141,   7  2');
  9   writeln(' 2');
 10   
 11   repeat
 12     write('   A,   n  3  ', lmax, ': ');
 13     read(n);
 14   until (n >= 3) and (n <= lmax);
 15   
 16   writeln(' :');
 17   for i := 1 to n do
 18     for j := 1 to n do
 19       read(A[i,j]);
 20 
 21   for i := 1 to n-1 do
 22     for j := i+1 to n do
 23       if A[1,i] > A[1,j] then
 24       begin
 25         t := A[1,i];
 26         A[1,i] := A[1,j];
 27         A[1,j] := t
 28       end;
 29   
 30   writeln('     : ',
 31       A[1,1], ' ', A[1,2], ' ', A[1,3]);
 32   
 33   writeln(' 3');
 34   nodd := 0;
 35   for i := 1 to n do
 36     if A[i, n-i+1] mod 2 <> 0 then
 37       nodd := nodd + 1;
 38   
 39   if nodd = 0 then
 40     writeln('  ')
 41   else
 42     writeln('    : ', nodd);
 43 end.
}}}
