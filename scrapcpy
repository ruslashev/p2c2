test/labs/lab2.pas ==================================================================
{{{
  1 program lab2;
  2 const lmax = 20;
  3 var R: array[1..lmax] of real;
  4     i, n, k, f, nmax: integer;
  5     a, x, h, max, avg: real;
  6 
  7 begin
  8   writeln('  2');
  9   writeln(' ,  155,  141, : 2, 7, 2');
 10   
 11   {  1 }
 12   writeln(' 1');
 13   repeat
 14     write('   n  1  ', lmax, ',   R: ');
 15     read(n);
 16   until (n > 0) and (n <= lmax);
 17   write('  a, x  h: ');
 18   read(a, x, h);
 19   
 20   for i := 1 to n do
 21     R[i] := 1.25*sin(3*a*x - i*h);
 22   
 23   writeln(' R:');
 24   for i := 1 to n do
 25     write(R[i]:-8:3);
 26   writeln;
 27   
 28   {  2 }
 29   f := 0;
 30   for i := n downto 1 do
 31     if R[i] < 0 then
 32       f := i;
 33   if f = 0 then
 34     writeln('    .')
 35   else
 36   begin
 37     writeln;
 38     writeln(' 2');
 39     
 40     k := 0;
 41     for i := 1 to n do
 42       if (i <= f) or (abs(R[i]) < 0.3) then
 43       begin
 44         k := k + 1;
 45         R[k] := R[i]
 46       end;
 47     
 48     writeln(' R'':');
 49     for i := 1 to k do
 50       write(R[i]:-8:3);
 51     writeln;
 52     if k = n then
 53       writeln(' ');
 54     
 55     {  3 }
 56     writeln;
 57     writeln(' 3');
 58     nmax := 0;
 59     max := -1e308;
 60     for i := 1 to k do
 61       if R[i] > max then
 62       begin
 63         nmax := i;
 64         max := R[i]
 65       end;
 66     
 67     if nmax = k then
 68       writeln('    ')
 69     else
 70     begin
 71       avg := 0;
 72       for i := nmax + 1 to k do
 73         avg := avg + R[i];
 74       avg := avg / (k - nmax);
 75       writeln(' : ', avg:-14:5)
 76     end
 77   end;
 78 end.
}}}
