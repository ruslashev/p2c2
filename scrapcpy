test/labs/lab5-2.pas ==================================================================
{{{
  1 program lab5_prog2;
  2 const lmax = 20;
  3 var i, j, n, m : integer; nr, mr, min, max : real;
  4     a : array [1..lmax, 1..lmax] of real;
  5 begin
  6   writeln('  5  2');
  7   writeln(' ,  155,  141,  2 ( 7)');
  8   
  9   repeat
 10     write(' n: ');
 11     read(nr);
 12   until (nr > 0) and (nr <= lmax) and (nr = round(nr));
 13   n := round(nr);
 14   repeat
 15      write(' m: ');
 16      read(mr);
 17   until (mr > 0) and (mr <= lmax) and (mr = round(mr));
 18   m := round(mr);
 19   
 20   writeln(' :');
 21   for i := 1 to n do
 22       for j := 1 to m do
 23        read(a[i,j]);
 24 
 25   max := a[1,1];
 26   for i := 1 to n do begin
 27     min := maxint;
 28     for j := 1 to m do
 29       if abs(a[i,j]) < abs(min) then
 30         min := a[i,j];
 31     if min > max then
 32       max := min;
 33   end;
 34 
 35   writeln('D = ', max);
 36 end.
}}}
