test/labs/lab4-1.pas ==================================================================
{{{
  1 program lab4_prog1;
  2 const lmax = 20;
  3 var A: array[1..lmax, 1..lmax] of integer;
  4     n, m, i, j, fz, lnz, t: integer;
  5     flag : boolean;
  6 
  7 begin
  8   writeln('  4  1');
  9   writeln(' ,  155,  141,   6');
 10   writeln(' 1');
 11   
 12   repeat
 13     write('  n  A[1:n,1:m],   2  ',
 14         lmax, ': ');
 15     read(n);
 16   until (n >= 2) and (n <= lmax);
 17   repeat
 18     write('  m,   1  ', lmax, ': ');
 19     read(m);
 20   until (m >= 1) and (m <= lmax);
 21   
 22   writeln(' :');
 23   for i := 1 to n do
 24     for j := 1 to m do
 25       read(A[i,j]);
 26   
 27   fz := 0;
 28   i := 1;
 29   flag := false;
 30   while (i <= n) and (flag = false) do begin
 31     for j := 1 to m do
 32       if A[i,j] = 0 then begin
 33         fz := i;
 34         flag := true;
 35       end;
 36     i := i + 1;
 37   end;
 38   lnz := n;
 39   flag := false;
 40   while (lnz >= 1) and (flag = false) do begin
 41     flag := true;
 42     for j := 1 to m do
 43       if A[lnz,j] = 0 then
 44         flag := false;
 45     if not flag then lnz := lnz - 1;
 46   end;
 47   
 48   if fz = 0 then
 49     writeln('    ')
 50   else if lnz = 0 then
 51     writeln('      ')
 52   else begin
 53     for j := 1 to m do begin
 54       t := A[fz,j];
 55       A[fz,j] := A[lnz,j];
 56       A[lnz,j] := t
 57     end;
 58     
 59     writeln(' A:');
 60     for i := 1 to n do begin
 61       for j := 1 to m do
 62         write(A[i,j]:3);
 63       writeln
 64     end
 65   end;
 66 end.
}}}
